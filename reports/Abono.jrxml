	<queryString>
		<![CDATA[SELECT
v.VentaID as NoFactura,
u.Nombre as Usuario,
c.Nombre as Cliente,
if(isnull(v.RTN) = 1, 'sin rtn', v.RTN) as RTN,
v.Fecha,
v.Hora,
p.ProductoID as Codigo,
p.Descripcion as Producto,
vd.Cantidad,
vd.Precio,
vd.ISV,
vd.Descuento
FROM Venta AS v
INNER JOIN VentaDetalle AS vd ON vd.VentaID = v.VentaID
INNER JOIN Producto as p ON p.ProductoID = vd.ProductoID
INNER JOIN Cliente as c ON c.ClienteID = if(isnull(v.ClienteID) = 1, 1, v.ClienteID)
INNER JOIN Usuario as u ON u.UsuarioID = v.UsuarioID
WHERE v.VentaID = $P{VentaID}]]>
	</queryString>
